QPushButton#noteButton, #rootButton {
    border: 2px solid #8f8f91;
    min-width: 50px;
}
QPushButton#rootButton {

    background-color: rgb(128, 0, 0);
    text-align: center;
    font-size: 24px;
    font: consolas;
    color: white;
    border-radius: 15px;
}
QPushButton#noteButton {

    text-align: center;
    font-size: 24px;
    font: consolas;
    color: white;
    border-radius: 15px;
}

QPushButton#button {

    background-color: rgb(128, 0, 0);

}
